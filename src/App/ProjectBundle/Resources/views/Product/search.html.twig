{% block title %}AppProjectBundle:Product:show{% endblock %}

{% block body %}

    <div class="row">
        <div class="span12">     
            {#AJAX JQUERY#}
            <div class="span3 col-lg-1 col-offset-6 centered">
                <form>
                    <br><input type="text" class="search_product" name="search_product">
                    <input type="submit">
                </form>
                <div class="product_result">
                </div>
            </div>
        </div> 
            
         {#AJAX XMLHttpRequest#}
        <div class="span12"> 
            <form>
                <br><input type="text" class="search_product_xhr" name="search_product">
            </form>
            <div id="product_result">
            </div>

            <div id='calendar'></div>
        </div>
        
        {#AJAX File Upload#}
        <div class="span12"> 
            <form>
                <br><input type="file" class="search_product_file" name="uploaded_file">
                <input type="submit" id="file_upload">
            </form>
            <div id="product_result">
            </div>

            <div id='calendar'></div>
        </div>
    </div>    
{% endblock %}
